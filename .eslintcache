),\n                {\n                    'Content-Type': 'application/json'\n                }\n            )\n            signin(responseData.adminData)\n            setShow(false)\n            setMessage(responseData.message)\n            enableDrawer(false)\n        } catch (err) {\n            console.log(err)\n        }\n    }\n\n    const onCancelHandler = () => {\n        setShow(false)\n        enableDrawer(false)\n    }\n\n    const onClearHandler = () => {\n        setMessage('')\n        enableDrawer(false)\n    }\n\n    const onClickHandler = () => {\n        setShow(true);\n        disableDrawer(true);\n    }\n\n\n    return (\n        <React.Fragment>\n            <ErrorModal error={error} onClear={clearError} />\n            <MessageModal\n                message={message}\n                onClear={onClearHandler}\n                className='admin-message-modal'\n            />\n            <Modal\n                show={show}\n                onCancel={onCancelHandler}\n                onSubmit={onSubmitHandler}\n                className='modal--auth'\n            >\n                <Input\n                    id='name'\n                    label='Fiók ID'\n                    onInput={inputHandler}\n                    value={inputState.inputs.name.value}\n                    errorText='Kérlek add meg a fiók ID-et'\n                    validators={[VALIDATOR_REQUIRE()]}\n                    type='text'\n                />\n                <Input\n                    id='password'\n                    label='Jelszó'\n                    onInput={inputHandler}\n                    value={inputState.inputs.password.value}\n                    errorText='Kérlek add meg a jelszavadat.'\n                    validators={[VALIDATOR_REQUIRE()]}\n                    type='password'\n                />\n                <button className='btn--save'>Mehet</button>\n            </Modal>\n            <div\n                onClick={!isLoggedIn ? onClickHandler :